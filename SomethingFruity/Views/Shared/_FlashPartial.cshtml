@foreach (var key in TempData.Keys)
{
    <script type="text/javascript">
        $(function() {
            @*toastr["@(key)"]("@TempData[key]");*@
            swal({
                    type: '@(key)',
                        title: '@TempData[key]',
                        showConfirmButton: true,
                        width: "60%"
                });
        });

    </script>
}